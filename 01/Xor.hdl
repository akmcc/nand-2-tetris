/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
  Not(in=a, out=nota);
  And(a=nota, b=b, out=w1);
  Not(in=b, out=notb);
  And(a=a, b=notb, out=w2);
  Or(a=w1, b=w2, out=out);
}